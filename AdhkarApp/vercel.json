{
  "version": 2,
  "name": "islamic-adhkar-ruqyah",
  "builds": [
    {
      "src": "www/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    },
    {
      "src": "/",
      "dest": "/www/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}